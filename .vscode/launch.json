        "PYTHONPATH": "C:/github/solliancenet/foundationallm/src/python//AgentHubAPI;C:/github/solliancenet/foundationallm/src/python//PythonSDK;C:/github/givenscj/langchain/libs/langchain"
      },
      "args": ["main:app", "--reload", "--port", "8742"]
    },
